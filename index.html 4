    <script>
        // Get the stream URL and extract the domain and path
        const streamUrl = new URL("");
        const domain = streamUrl.hostname;
        const pathUrl = streamUrl.pathname + streamUrl.search;

        // Dynamically assign the stream path based on domain
        let stream;
        if (domain === "app.jagobd.com.bd") {
            stream = "/stream/" + pathUrl;
        } else {
            stream = "";
        }

        // Detect if the stream is an MPD (DASH) or M3U8 (HLS) file
        const streamType = stream.endsWith('.mpd') ? 'dash' : 'hls';
        let drmConfig = {};

        // Check if streamType is 'dash' and set DRM configuration
        if (streamType === 'dash') {
            drmConfig = {
                clearkey: {
                    key: "",  // Replace with your ClearKey DRM key
                    keyId: ""  // Replace with your ClearKey DRM key ID
                }
            };
        }

        // JW Player setup object
        const playerSetup = {
            file: stream,
            type: streamType,  // Set the type (dash or hls)
            plugins: [HlsjsPlayback, LevelSelector, AudioTrackSelector],
            width: "100%",
            aspectratio: "16:9",
            autostart: true,
            mute: false,
            playbackRateControls: [0.5, 1, 1.5, 2],
            visualplaylist: true,
            qualityLabels: {
                144: '144p',
                240: '240p',
                360: '360p',
                480: '480p',
                720: '720p',
                1080: '1080p'
            }
        };

        // Add DRM config to playerSetup if streamType is 'dash'
        if (streamType === 'dash') {
            playerSetup.drm = drmConfig;
        }

        // Initialize JW Player
        jwplayer("myPlayer").setup(playerSetup);
        jwplayer.key = 'Khpp2dHxlBJHC8MCmLnBuV2jK/DwDnJMniwF6EO9HC/riJ712ZmbHg==';
    </script>
</div>

